{% extends ':Layout:layout.html.twig' %}

{% set module = 'project' %}

{% block content_title %}
    Projects
{% endblock %}

{% block content_body %}
    <div class="box box-primary">
        <div class="box-header">
            {% if entity is null %}New Project{% else %}{{ entity.name }}{% endif %}
        </div>
        <div class="box-body">
            {{ form_start(form) }}
                {{ form_row(form.name) }}
                {{ form_row(form.client) }}
                {{ form_row(form.submit) }}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}